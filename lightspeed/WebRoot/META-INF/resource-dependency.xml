<?xml version="1.0"?>
<!DOCTYPE xml>

<dependencies>
	<!-- Load the ACE component CSS files before the Lightspeed CSS, so that LS will override the builtin styles -->
	<resource name="global.css">
		<requires>
			<resource name="util/combined.css" library="icefaces.ace" />
			<resource name="core.css" library="ice.core" />
			<resource name="fontawesome/font-awesome.css" library="icefaces.ace" />
		</requires>
	</resource>
</dependencies>

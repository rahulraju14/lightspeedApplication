		class="com.lightspeedeps.message.MessageHandler">
		<description>Bean for sending messages</description>
		<property name="messageDAO" ref="MessageDAO" />
		<property name="taskExecutor" ref="taskExecutor" />
	</bean>

	<bean id="NotificationBean"
		class="com.lightspeedeps.message.DoNotification">
		<description>Bean for sending notification messages</description>
	</bean>

	<bean id="ExecuteReport"
		class="com.lightspeedeps.batch.ExecuteReport">
		<description>Bean for scheduling and running asynchronous report
			generation</description>
		<property name="sessionFactory" ref="sessionFactory" />
		<property name="taskExecutor" ref="taskExecutor" />
	</bean>

	<!-- runSomething is only used by developers, not in production -->
	<bean id="runSomething"
		class="com.lightspeedeps.test.RunSomething">
		<property name="projectDAO" ref="ProjectDAO" />
		<property name="userDAO" ref="UserDAO" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<!-- The remainder of the file duplicates the first part of applicationContextScheduler.xml, 
		except that parameters here may not refer to values defined in the web.xml 
		file. -->

	<bean id="mailSender"
		class="org.springframework.mail.javamail.JavaMailSenderImpl">
		<description>Utility bean for email support</description>
		<property name="host" value="smtp.**some server**" />
		<property name="port" value="587" /><!-- 587 for gmail, 25 for 
			others -->
		<property name="username" value="**someone@some server.com" />
		<property name="password" value="**password**" />
	</bean>

	<bean id="appMailer" class="com.lightspeedeps.message.Mailer">
		<description>Utility bean for email support</description>
		<property name="mailSender" ref="mailSender" />
		<property name="from" value="dwight@lightspeedeps.com" />
	</bean>

	<!-- VARIOUS SCHEDULED JOBS -->

	<!-- Define the beans to be run -->

	<bean id="clearDoodMapJob"
		class="com.lightspeedeps.batch.ClearDoodMapJob">
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="dueOverdueCheck"
		class="com.lightspeedeps.batch.DueOverdueCheck">
		<property name="callsheetDAO" ref="CallsheetDAO" />
		<property name="dprDAO" ref="DprDAO" />
		<property name="exhibitGDAO" ref="ExhibitGDAO" />
		<property name="notificationDAO" ref="NotificationDAO" />
		<property name="projectDAO" ref="ProjectDAO" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="eventLogSummary"
		class="com.lightspeedeps.batch.EventLogSummary">
		<property name="triggerHours" value="2" /><!-- best if this matches 
			the quartz schedule -->
		<property name="mailer" ref="appMailer" />
		<property name="recipient" value="dwight@lightspeedeps.com" />
		<property name="eventDAO" ref="EventDAO" />
		<property name="productionDAO" ref="ProductionDAO" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="failedLogonJob"
		class="com.lightspeedeps.batch.FailedLogonJob">
		<property name="mailer" ref="appMailer" />
		<property name="recipient" value="dwight@lightspeedeps.com" />
		<property name="userDAO" ref="UserDAO" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="formI9Check" class="com.lightspeedeps.batch.FormI9Check">
		<property name="notificationDAO" ref="NotificationDAO" />
		<property name="projectDAO" ref="ProjectDAO" />
		<property name="authBean" ref="AuthBean" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="scriptPagesCheck"
		class="com.lightspeedeps.batch.ScriptPagesCheck">
		<property name="notificationDAO" ref="NotificationDAO" />
		<property name="projectDAO" ref="ProjectDAO" />
		<property name="projectMemberDAO" ref="ProjectMemberDAO" />
		<property name="sceneDAO" ref="SceneDAO" />
		<property name="stripDAO" ref="StripDAO" />
		<property name="authBean" ref="AuthBean" /><!-- JUnit: use ref='AuthBean' 
			not 'authBean' -->
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="timecardCreator"
		class="com.lightspeedeps.batch.TimecardCreator">
		<property name="startFormDAO" ref="StartFormDAO" />
		<property name="weeklyTimecardDAO" ref="WeeklyTimecardDAO" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<bean id="databaseCheck"
		class="com.lightspeedeps.batch.DatabaseCheck">
		<property name="productionDAO" ref="ProductionDAO" />
		<property name="sessionFactory" ref="sessionFactory" />
	</bean>

	<!-- import resource="applicationContextMongo.xml"/><!- - Mongo related 
		DAOs beans and -->
	<mongo:mongo-client id="mongo" host="localhost"
		port="27017"><!-- "172.16.10.37" for Team -->
		<mongo:client-options
			write-concern="ACKNOWLEDGED" />
	</mongo:mongo-client>

	<bean id="mongoTemplate"
		class="org.springframework.data.mongodb.core.MongoTemplate">
		<constructor-arg ref="mongo" />
		<!-- Change 'value' in next line to use a different MongoDB database on 
			same host (server) -->
		<constructor-arg name="databaseName"
			value="lightspeed" />
	</bean>

	<bean id="ContentDAO" class="com.lightspeedeps.dao.ContentDAO">
		<property name="mongoTemplate">
			<ref bean="mongoTemplate" />
		</property>
	</bean>

	<bean id="XfdfContentDAO"
		class="com.lightspeedeps.dao.XfdfContentDAO">
		<property name="mongoTemplate">
			<ref bean="mongoTemplate" />
		</property>
	</bean>

</beans>
